{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "regions": ["sin1"],
  "functions": {
    "app/**": {
      "maxDuration": 10
    }
  },
  "github": {
    "autoAlias": true,
    "autoJobCancelation": true
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  }
}